<div class="form">
<%= form_for agent ,remote: true do |f| %>
<h4 class="text-center mb-2">Agent Details</h4>
  <div class="form-group">
    <%= f.label :picture %>
    <%= f.file_field :picture , class: 'form-control'%>
  </div>

  <div class="form-group">
    <%= f.label :name %>
    <%= f.text_field :name,value:current_user.name, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.label :email %>
    <%= f.email_field :email,value:current_user.email, class: 'form-control' %>
  </div>

  <div class="form-group">
    <%= f.label :phone %>
    <%= f.number_field :phone, class: 'form-control' %>
  </div>
  <div class="field">

    <%= f.hidden_field :property_id, value: property_id %>
  </div>

  <div class="actions">
    <%= f.submit("Next",class:"btn btn-primary float-right")%>
  </div>
<% end %>
</div>
